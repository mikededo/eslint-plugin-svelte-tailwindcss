{
  "name": "eslint-plugin-svelte-tailwindcss",
  "type": "module",
  "version": "1.1.0",
  "description": "ESLint plugin for Svelte and Tailwind CSS",
  "author": "Miquel De Domingo",
  "license": "MIT",
  "homepage": "https://github.com/mikededo/eslint-plugin-svelte-tailwindcss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikededo/eslint-plugin-svelte-tailwindcss.git"
  },
  "bugs": "https://github.com/mikededo/eslint-plugin-svelte-tailwindcss/issues",
  "keywords": [
    "eslint",
    "eslintplugin",
    "svelte",
    "tailwindcss",
    "tailwind",
    "tailwindcss-svelte",
    "eslint-plugin-svelte",
    "eslint-plugin-tailwindcss",
    "eslint-plugin-tailwind",
    "eslint-plugin-tailwindcss-svelte"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "gen:rule": "pnpm run scripts/rule.ts",
    "gen:update": "pnpm run scripts/update.ts",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "test": "vitest .",
    "test:cov": "vitest . --coverage",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "^9.0.0 || ^10.0.0"
  },
  "dependencies": {
    "clear-module": "4.1.2",
    "enhanced-resolve": "5.18.3",
    "escalade": "3.2.0",
    "jiti": "2.6.0",
    "postcss": "8.5.6",
    "postcss-import": "16.1.1",
    "synckit": "0.11.11",
    "tailwindcss": "3.4.16"
  },
  "devDependencies": {
    "@types/eslint": "9.6.1",
    "@types/node": "24.5.2",
    "@types/postcss-import": "14.0.3",
    "@typescript-eslint/rule-tester": "8.44.1",
    "@typescript-eslint/scope-manager": "8.44.1",
    "@typescript-eslint/types": "8.44.1",
    "@typescript-eslint/utils": "8.44.1",
    "@vitest/coverage-v8": "3.2.4",
    "commander": "14.0.1",
    "eslint": "9.36.0",
    "eslint-plugin-format": "1.0.2",
    "eslint-plugin-svelte": "3.12.4",
    "jq": "1.7.2",
    "svelte": "5.39.5",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "unbuild": "3.6.1",
    "vite": "7.1.7",
    "vitest": "3.2.4"
  }
}
