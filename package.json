{
  "name": "eslint-plugin-svelte-tailwindcss",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "bun@1.1.33",
  "description": "ESLint plugin for Svelte and Tailwind CSS",
  "author": "Miquel De Domingo",
  "license": "MIT",
  "homepage": "https://github.com/mikededo/eslint-plugin-svelte-tailwindcss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikededo/eslint-plugin-svelte-tailwindcss.git"
  },
  "bugs": "https://github.com/mikededo/eslint-plugin-svelte-tailwindcss/issues",
  "keywords": [
    "eslint",
    "eslintplugin",
    "svelte",
    "tailwindcss",
    "tailwind",
    "tailwindcss-svelte",
    "eslint-plugin-svelte",
    "eslint-plugin-tailwindcss",
    "eslint-plugin-tailwind",
    "eslint-plugin-tailwindcss-svelte"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "gen:rule": "bun run scripts/rule.ts",
    "gen:update": "bun run scripts/update.ts",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "test": "vitest . --watch=false",
    "test:cov": "vitest . --coverage --watch=false",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "dependencies": {
    "tailwindcss": "3.4.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.8.0",
    "@changesets/cli": "2.27.9",
    "@sveltejs/vite-plugin-svelte": "4.0.0",
    "@svitejs/changesets-changelog-github-compact": "1.2.0",
    "@types/bun": "latest",
    "@types/eslint": "9.6.1",
    "@types/node": "22.9.0",
    "@typescript-eslint/parser": "8.14.0",
    "@vitest/coverage-v8": "2.1.5",
    "commander": "12.1.0",
    "eslint": "9.14.0",
    "eslint-plugin-format": "0.1.2",
    "svelte": "5.1.15",
    "svelte-eslint-parser": "0.43.0",
    "typescript": "5.6.3",
    "unbuild": "2.0.0",
    "vite": "5.4.11",
    "vitest": "2.1.4"
  }
}
